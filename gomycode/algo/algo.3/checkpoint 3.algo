FUNCTION insertionsort(VAR arr: ARRAY_OF INTEGER[], n : INTEGER) : ARRAY_OF INTEGER 
VAR
    j,i,k : INTEGER ;
BEGIN
    FOR i FROM 1 TO n-1 STEP step 1  DO
        k := arr[i] ;
        j := i - 1;
        WHILE (j >= 0 and arr[j]>k) DO
            arr[j+1] = arr[j];
            j = j-1;
        END_WHILE
        arr[j+1] = k;
    END_FOR
    RETURN arr ;
END


